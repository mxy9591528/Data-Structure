#include<stdio.h>
#include<stdlib.h>
struct LinkList
{
    int n;
    struct LinkList*next;
};
int main()
{
    int num;
    scanf("%d",&num);
    struct LinkList*head=NULL,*end=NULL;
    for(int i=0;i<num;i++)
    {
        struct LinkList*node=(struct LinkList*)malloc(sizeof(struct LinkList));
        scanf("%d",&node->n);
        node->next=NULL;
        if(!head)
        {
            head=node;
            end=node;
        }
        else
        {
            end->next=node;
            end=node;
        }
    }
    int k;
    scanf("%d",&k);
    struct LinkList *prev=NULL;
    struct LinkList *cur=head;
    while(cur)
    {
        if(cur->n==k)
        {
            struct LinkList *temp=cur;
            if(cur==head)
            {
                head=head->next;
                cur=head;
            }
            else
            {
                prev->next=cur->next;
                cur=prev->next;
            }
            free(temp);
            num--;
        }
        else
        {
            prev=cur;
            cur=cur->next;
        }
    }
    printf("%d\n",num);
    cur=head;
    while(cur)
    {
        printf("%d ",cur->n);
        cur=cur->next;
    }
    cur=head;
    struct LinkList*del=NULL;
    while(cur)
    {
        del=cur;
        cur=cur->next;
        free(del);
    }
    head=NULL;
    return 0;
}
